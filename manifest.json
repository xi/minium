{
  "manifest_version": 2,

  "name": "Minium",
  "description": "Minimal Vimium; only implementing few keybindings",
  "version": "1.0",

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": [
      "minium.js"
    ],
    "run_at": "document_start",
    "all_frames": true
  }],

  "permissions": [
    "<all_urls>"
  ]
}
